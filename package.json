{
  "name": "gatsby-plugin-react-helmet-canonical-urls",
  "version": "1.4.0",
  "description": "Add canonical links to HTML pages Gatsby generates using React Helmet.",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore src/*.test.js",
    "watch": "babel -w src --out-dir . --ignore src/*.test.js",
    "format": "prettier --write 'src/**/*.js'",
    "prepare": "cross-env NODE_ENV=production yarn run build",
    "test": "jest --env node"
  },
  "repository": "https://github.com/NickCis/gatsby-plugin-react-helmet-canonical-urls",
  "author": "Nicolas Cisco <ncis20@gmail.com>",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-react-helmet",
    "seo",
    "canonical",
    "gatsby-plugin-canonical-urls"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "babel-preset-gatsby-package": "^0.1.3",
    "cross-env": "^5.2.0",
    "jest": "^24.7.1",
    "prettier": "^1.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0"
  },
  "peerDependencies": {
    "gatsby-plugin-react-helmet": "*",
    "react-helmet": "*"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "minimatch": "^3.0.4"
  }
}
